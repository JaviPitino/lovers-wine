<h2>wines-create</h2>



<form action="/wines/create" method="POST">
<label for="nombre">Nombre: </label>
<input type="text" name="nombre">
<br>
<label for="tipoVino">Tipo Vino: </label>
<select name="tipoVino" multiple="true">
    {{#each foundVino}}
     <option value="{{this._id}}">{{this.tipoVino}}</option>
     {{/each}}
</select>
<br>
<label for="anada">Añada: </label>
<select name="anada" multiple="true">
    {{#each foundVino}}
     <option value="{{this._id}}">{{this.anada}}</option>
     {{/each}}
</select>
<br>
<label for="ano">Año: </label>
<input type="text" name="ano">
<br>
<label for="denOrigen">Denominación de Origen: </label>
<input type="text" name="denOrigen">
<br>
<label for="puntuacion">Puntuación: </label>
<input type="text" name="puntuacion">

<form action="/wines/create" method="POST" enctype="multipart/form-data">

  <label for="image"> Imagen: </label>
  <input type="file" name="image">

  <button type="submit"> Añadir Imagen </button>
</form>

<button> Añadir Vino </button>

</form>

